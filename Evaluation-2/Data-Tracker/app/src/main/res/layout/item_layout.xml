<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_margin="10dp"
    android:id="@+id/cardview"
    app:cardCornerRadius="40dp"
    android:background="@drawable/ic_rectangle_8"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_margin="20dp"
        android:orientation="vertical"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Date"
            android:textSize="20dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="30-10-2021"
            android:id="@+id/tvDate"
            android:textSize="25dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="postive"
            android:textSize="20dp"/>
        <TextView
            android:id="@+id/tvPositve"
            android:layout_width="wrap_content"
            android:text="28756489"
            android:textSize="25dp"
            android:layout_below="@id/tvDate"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Negative"
            android:layout_marginTop="20dp"
            android:textSize="20dp"/>

        <TextView
            android:id="@+id/tvNegative"
            android:layout_width="wrap_content"
            android:text="28756489"
            android:textSize="25dp"
            android:layout_below="@id/tvDate"
            android:layout_height="wrap_content" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="hospitalizedCurrently"
            android:textSize="20dp"/>
        <TextView
            android:id="@+id/tvhospitalizedCurrently"
            android:layout_width="wrap_content"
            android:text="40199"
            android:textSize="25dp"
            android:layout_below="@id/tvDate"
            android:layout_height="wrap_content" />



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="onVentilatorCurrently"
            android:layout_marginTop="20dp"
            android:textSize="20dp"/>

        <TextView
            android:id="@+id/tvonVentilatorCurrently"
            android:layout_width="wrap_content"
            android:text="40199"
            android:textSize="25dp"
            android:layout_below="@id/tvDate"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="death"
            android:textSize="20dp"/>

        <TextView
            android:id="@+id/tvdeath"
            android:layout_width="wrap_content"
            android:text="40199"
            android:textSize="25dp"
            android:layout_below="@id/tvDate"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Date formated"
            android:textSize="20dp"/>

        <TextView
            android:id="@+id/tvdateChecked"
            android:layout_width="wrap_content"
            android:text="40199"
            android:textSize="25dp"
            android:layout_below="@id/tvDate"
            android:layout_height="wrap_content" />


    </LinearLayout>
</androidx.cardview.widget.CardView>